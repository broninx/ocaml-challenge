(library
 (name opam-format)
 (kind normal)
 (synopsis "OCaml Package Manager file format handling library")
 (archives (byte opam_format.cma) (native opam_format.cmxa))
 (plugins (byte opam_format.cma) (native opam_format.cmxs))
 (native_archives opam_format.a)
 (requires opam-core opam-file-format re)
 (modes byte native)
